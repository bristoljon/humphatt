{
  "name": "yarsk",
  "version": "1.0.0",
  "description": "My React starter kit. Webpack, Babel, Sass, React 0.14 and more.",
  "main": "app/index.js",
  "scripts": {
    "lint:js": "eslint ./app ./conf ./build-gh-pages.js",
    "karma": "cross-env NODE_ENV=test karma start",
    "test": "jest --verbose",
    "coverage": "jest --coverage",
    "start": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "build": "cross-env NODE_ENV=production webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist"
  },
  "author": "Brad Daily <brad@bradleyboy.com>",
  "license": "MIT",
  "dependencies": {
    "firebase": "^3.3.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-jsx": "^1.2.2",
    "eslint-config-standard-react": "^2.5.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.3",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  }
}
